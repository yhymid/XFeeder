{
  "Settings": {
    "Logs": false
  },

  "Auth": {
    "Token": "${DISCORD_AUTH_TOKEN}",
    "x-super-properties": "${DISCORD_X_SUPER_PROPERTIES}",
    "cookie": "${DISCORD_COOKIE}"
  },

  "Workshop": {
    "Enabled": false,
    "Dir": "src/workshop"
  },

  "Proxy": {
    "Enabled": false,
    "Url": "http://127.0.0.1:8080"
  },

    "Scrapling": {
    "Enabled": true,
    "ForceGlobal": false,
    "Comment": "Requires: pip install 'scrapling[all]' && scrapling install",
    "AutoFallback": true,
    "PreferredMode": "cli",
    "DefaultFetchMode": "stealthy-fetch",
    "FallbackStatusCodes": [403, 429, 503],
    
    "DefaultOptions": {
      "headless": true,
      "solveCaptcha": false,
      "timeout": 60,
      "wait": 0,
      "disableResources": true,
      "networkIdle": false,
      "waitSelector": null,
      "waitSelectorState": null,
      "blockedDomains": [],
      "retries": 3,
      "retryDelay": 1
    },
    
    "PerUrlConfig": {
      "https://protected-site.com/feed": {
        "enabled": true,
        "mode": "stealthy-fetch",
        "solveCaptcha": true,
        "networkIdle": true,
        "waitSelector": ".content-ready",
        "waitSelectorState": "visible",
        "blockedDomains": ["ads.example.com", "tracker.example.com"],
        "retries": 3,
        "retryDelay": 1,
        "htmlParseOptions": {
          "itemSelector": "article",
          "titleSelector": "h2",
          "linkSelector": "a.read-more"
        }
      },
      "https://some-cloudflare-site.com": {
        "enabled": true,
        "mode": "stealthy-fetch",
        "solveCaptcha": true,
        "networkIdle": true,
        "wait": 1500,
        "timeout": 90,
        "retries": 5,
        "retryDelay": 2
      }
    }
  },

  "channels": [
    {
      "Webhook": "${DISCORD_WEBHOOK_MAIN}",
      "Thread": null,
      "RSS": [
        "https://example.com/rss",
        "https://example.org/feed.xml",
        "https://www.youtube.com/feeds/videos.xml?channel_id=UC_CHANNEL_ID"
      ],
      "TimeChecker": 30,
      "RequestSend": 5
    }
  ],

  "channels2": [
    {
      "Webhook": "${DISCORD_WEBHOOK_MAIN}",
      "Thread": null,
      "RSS": [
        "https://github.com/user/repo/commits.atom"
      ],
      "TimeChecker": 60,
      "RequestSend": 2
    },
    {
      "Discord": {
        "GuildID": "YOUR_GUILD_ID",
        "Webhook": "${DISCORD_WEBHOOK_MAIN}",
        "Thread": null,
        "Limit": 5,
        "ChannelIDs": ["CHANNEL_ID_1", "CHANNEL_ID_2"],
        "TimeChecker": 30,
        "RequestSend": 1
      }
    }
  ]
}
